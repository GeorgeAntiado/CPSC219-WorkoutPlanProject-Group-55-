<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.WorkoutplanController">
   <children>
      <Label text="Welcome To The Workout Plan Home Page:">
         <VBox.margin>
            <Insets bottom="20.0" left="70.0" right="70.0" top="10.0" />
         </VBox.margin>
         <font>
            <Font name="Bauhaus 93" size="26.0" />
         </font></Label>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label text="To find your daily calorie needs:">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="15.0" />
               </VBox.margin>
               <font>
                  <Font name="Calibri" size="16.0" />
               </font>
            </Label>
            <Button mnemonicParsing="false" onAction="#switchToCalorieCalculatorScene" text="Calorie Counter">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin></Button>
         </children>
      </VBox>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label text="Meal Schedule:">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
               <font>
                  <Font name="Calibri" size="16.0" />
               </font></Label>
            <Button mnemonicParsing="false" onAction="#switchToWeeklyEatingPlan" text="Meal Schedule">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin></Button>
         </children>
      </VBox>
   </children>
</VBox>
